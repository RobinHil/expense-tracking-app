     <property name="styleSheet">
      <string notr="true">QComboBox {
  border: 2px solid #204a87; /* Blue border */
  border-radius: 5px;
  padding: 5px;
  padding-right: 20px; /* Make room for the icon */
  background-color: #000000; /* Black background */
