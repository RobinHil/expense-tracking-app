}

QComboBox::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 15px;
  border-left-width: 1px;
  border-left-color: #204a87;
  border-left-style: solid; /* just a single line */
  border-top-right-radius: 3px; /* same radius as the QComboBox */
  border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
  image: url(:/icons/down-arrow.png); /* Replace with the actual path to your down arrow icon */
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
  top: 1px;
  left: 1px;
}

QComboBox:hover { /* Hover effect for the QComboBox itself */
  background-color: #333333; /* Dark grey background */
}

QComboBox::right-icon {
  image: url(:/icons/star.png); /* Replace with the actual path to your star icon */
  position: absolute;
  top: 1px;
  right: 3px;
  width: 16px;
  height: 16px;
}

QComboBox QAbstractItemView {
  border: 2px solid #204a87;
  selection-background-color: #204a87; /* Blue background for the selected item */
  background-color: #000000; /* Black background */
  color: white; /* White text color */
}

QComboBox QAbstractItemView::item {
  background-color: #000000; /* Black background for each item */
  color: white; /* White text color for each item */
  padding: 2px; /* Optional: add padding for each item */
}

ComboBox QAbstractItemView::item:hover {
  /* Hover effect for individual items in the dropdown list */
  background-color: white; /* White background for hovered item */
  color: black; /* Black text color for hovered item */
}

QComboBox QAbstractItemView::item:selected {
  background-color: #204a87; /* Blue background for the selected item */
  color: white; /* White text color for the selected item */
}

